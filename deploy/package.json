{
    "name": "playground-contract",
    "version": "1.0.0",
    "scripts": {
        "keygen": "evdevkit keygen",
        "build": "evdevkit bundle ../contract/dist $EV_USER_PUBLIC_KEY /usr/bin/node -a index.js",
        "acquire": "evdevkit acquire > instance.json 2> error.log",
        "deploy": "evdevkit deploy ../contract/bundle/bundle.zip $EV_USER_IP $EV_USER_PORT"
    },
    "dependencies": {
        "@vercel/ncc": "^0.36.1",
        "ever-lmdb-sdk": "^1.0.10-alpha.1",
        "hotpocket-nodejs-contract": "^0.5.7",
        "node-lmdb": "^0.9.7"
    }
}
